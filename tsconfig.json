{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/about-us/page.jsx", "src/app/contact-us/page.jsx", "src/app/for-attorneys/page.jsx", "src/components/for-attorneys/AttorneyReviews.jsx", "src/components/for-attorneys/FAQ.jsx", "src/components/for-attorneys/FeatureBenefits.jsx", "src/components/for-attorneys/Hero.jsx", "src/components/for-attorneys/HowItWorks.jsx", "src/components/for-attorneys/Statistics.jsx", "src/components/for-attorneys/ValueProposition.jsx", "src/components/for-attorneys/Testimonials.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
